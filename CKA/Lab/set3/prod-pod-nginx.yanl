apiVersion: v1
kind: Pod
metadata:
  labels:
    run: prod-pod-nginx
  name: prod-pod-nginx
spec:
  tolerations:
  - key: "env_type"
    operator: "Equal"
    value: "production"
    effect: "NoSchedule"
  containers:
  - image: nginx
    name: prod-pod-nginx
    resources: {}
  dnsPolicy: ClusterFirst
  restartPolicy: Always
status: {}
